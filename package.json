{
  "name": "vc-notice",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Yusuke4869",
  "scripts": {
    "dev": "ts-node-dev src/main.ts",
    "dev:commands": "ts-node-dev src/commandsRegistration/deploy.ts",
    "build": "rm -rf dist/ && tsc",
    "start": "node dist/main.js",
    "start-vc-notice": "node dist/main.js",
    "deploy:commands": "node dist/commandsRegistration/deploy.js",
    "lint": "eslint --ignore-path .gitignore src/**/*.ts",
    "lint:fix": "eslint --ignore-path .gitignore src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "check": "prettier --check src/**/*.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "yarn lint:fix",
      "yarn format"
    ]
  },
  "packageManager": "yarn@4.1.0",
  "volta": {
    "node": "20.11.1"
  },
  "dependencies": {
    "discord-api-types": "^0.37.0",
    "discord.js": "^14.0.0",
    "dotenv": "^16.0.1",
    "mongodb": "^6.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.18.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
